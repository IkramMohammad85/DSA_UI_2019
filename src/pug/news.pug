                $("#siimage").attr("src", "https://www.dezshira.com/includes/modules/securimage/securimage_show_mini.php?"+d.getTime());
            });
        
        $("#btn_mini_contact_form").click(function() {
            $('html, body').animate({
                scrollTop: ($("#mini_contact_form").offset().top)-300
            }, 2000);
        });
        

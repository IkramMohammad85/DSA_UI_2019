script.
    // This example displays a marker at the center of Australia.
    // When the user clicks the marker, an info window opens.

    function initMap() {
    var uluru = {lat: -25.363, lng: 131.044};
    var map = new google.maps.Map(document.getElementById('aboutmap'), {
        zoom: 4,
        center: uluru
    });

    var contentString = '<div class="hAbout__map__content"><p>Country, city, street, house 12</p><p><a href="tel:+1 (234) 567-89-90">+1 (234) 567-89-90</a></p><p><a href="mailto:info@collector.com">info@collector.com</a></p></div>';

    var infowindow = new google.maps.InfoWindow({
        content: contentString,
        maxWidth: 210
    });

    var marker = new google.maps.Marker({
        position: uluru,
        map: map,
        icon: '/images/icons/pin.png',
    });
    
    marker.addListener('click', function() {
        infowindow.open(map, marker);
    });

    infowindow.open(map, marker);
    }
script(async='', defer='', src='https://maps.googleapis.com/maps/api/js?key=AIzaSyCWICyQYOBw5B_UKutiZlhKFfCRbWhrZ6A&callback=initMap')